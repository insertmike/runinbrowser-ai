{
  "installCommand": "cd ../.. && npm install && npm run build && cd examples/pocket-brain && npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "redirects": [
    {
      "source": "/chat",
      "destination": "/app",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
